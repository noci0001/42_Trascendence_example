{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": [
      "ts-jest",
      {
        "astTransformers": {
          "before": ["test/enable-plugin.ts"]
        }
      }
    ]
  },
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/../src/$1"
  }
}
